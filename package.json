{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:hot": "webpack serve --env port=3000 refresh=hot",
    "start:dev:server": "node ./json-server/index.js",
    "start:dev": "concurrently -c bgRed \"npm run start:hot\" \"npm run start:dev:server\" \"npm run storybook\"",
    "build:prod": "webpack --env mode=production apiUrl=https://blog-fake-server.vercel.app",
    "build:dev": "webpack --env mode=development",
    "exp:ico": "node scripts/export-icons.js",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"src/**/*.css\"",
    "lint:css:fix": "npx stylelint \"src/**/*.css\" --fix",
    "storybook": "start-storybook -p 6006 -c ./config/storybook --no-open",
    "storybook:build": "build-storybook -c ./config/storybook",
    "test:unit": "jest --config config/jest/jest.config.ts",
    "test:ui": "npx loki test --chromeDockerImage=yukinying/chrome-headless-browser-stable:100.0.4896.127",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:up": "npx loki update --chromeDockerImage=yukinying/chrome-headless-browser-stable:100.0.4896.127",
    "test:ui:ci": "loki --chromeDockerImage=yukinying/chrome-headless-browser-stable:100.0.4896.127 --requireReference --reactUri file:./storybook-static"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
